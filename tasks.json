{
    "tasks": [
        {
            "type": "shell",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
				"-Wall",
				"-Wextra",
				"-Werror",
                "-g",
				"-I${workspaceFolder}/include/",
				"-I${workspaceFolder}/libft/",
				"-I${workspaceFolder}/mlx_opengl/",
				"game_Macos.c",
                "${file}",
				"${fileDirname}/fill.c",
                "init.c",
				"map.c",
				"panel.c",
				"player.c",
				"validate.c",
				"utils/file_util.c",
				"utils/game_util.c",
				"utils/img_util.c",
				"${fileDirname}/utils/tile_util.c",
				"${fileDirname}/utils/vector_util.c",
				"${workspaceFolder}/libft/libft.a",
				"${workspaceFolder}/mlx_opengl/libmlx.a",
				"-lft",
				"-L${workspaceFolder}/libft",
				"-L${workspaceFolder}/mlx_opengl",
				"-lmlx",
				"-lm",
				"-lz",
				"-o",
                "${workspaceFolder}/${workspaceFolderBasename}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}